package stringDemo;

public class Demo1 {

	public static void main(String[] args) {

		String s1 = "Rama";// Type 1-->if strings are created with type 1 then the memory is allocated in
							// constant pool of String pool
		String s2 = new String("Laxman"); /* Type 2 --->strings are created using type 2 ,memory is allocated in the non
											 constant pool of String pool...Duplicates are allowed */
		System.out.println(s1 + s2);
		

	}

}
